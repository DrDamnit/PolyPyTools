#!/usr/bin/env bash

#Files for installation
INSTALL=( check-everyone.py check-registered.py configline.py gendir.py makecfg.py randomizepw.py restart-polycom.sh )

#Installed name
INSTALLAS=( check-everyone check-registered configline gendir makecfg randomizepw restart-polycoms )

